{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["@babel/env", {
          "targets": { "node":"current" }
        }]
      ]
    },
    "renderer": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }]
      ],
      "plugins": [
        "babel-plugin-transform-vue-jsx",
        "@babel/plugin-transform-runtime" // 处理regeneratorRuntime is not defined问题
      ]
    },
    "web": {
      "presets": [
        ["@babel/env", {
          "modules": false
        }]
      ]
    }
  },
  "plugins": ["@babel/plugin-proposal-function-bind", "@babel/plugin-syntax-dynamic-import"]
}
